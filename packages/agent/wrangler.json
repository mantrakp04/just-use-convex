{
  "name": "just-use-convex-agent-worker-barrel",
  "main": "src/index.ts",
  "compatibility_date": "2026-01-20",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env"
  ],
  "vars": {
    "NODE_ENV": "development",
    "SANDBOX_BUCKET_NAME": "sandboxes",
    "SANDBOX_BUCKET_ENDPOINT": "http://localhost:9000",
    "SANDBOX_BUCKET_ACCESS_KEY_ID": "minioadmin",
    "SANDBOX_BUCKET_SECRET_ACCESS_KEY": "minioadmin",
    "SANDBOX_ROOT_DIR": "/workspace"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "agentWorker",
        "class_name": "AgentWorker",
        "script_name": "just-use-convex-agent-worker-barrel"
      },
      {
        "name": "Sandbox",
        "class_name": "Sandbox"
      }
    ]
  },
  "r2_buckets": [
    {
      "binding": "SandboxBucket",
      "bucket_name": "just-use-convex-sandboxes",
      "preview_bucket_name": "just-use-convex-sandboxes"
    }
  ],
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "AgentWorker"
      ],
      "new_classes": [
        "Sandbox"
      ]
    }
  ],
  "containers": [
    {
      "class_name": "Sandbox",
      "image": "docker.io/cloudflare/sandbox:0.7.0"
    }
  ]
}
